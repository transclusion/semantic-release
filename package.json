{
  "name": "@transclusion/semantic-release",
  "version": "0.2.5",
  "main": "lib/main.js",
  "repository": "git@github.com:transclusion/semantic-release.git",
  "author": "Marius Lundg√•rd <studio@mariuslundgard.com>",
  "license": "MIT",
  "bin": {
    "semantic-release": "bin/semantic-release"
  },
  "files": [
    "bin",
    "lib"
  ],
  "pre-commit": [
    "flow-check",
    "lint",
    "test"
  ],
  "scripts": {
    "build": "yarn build-src && yarn build-src-flow",
    "build-src": "babel src --out-dir lib",
    "build-src-flow": "flow-copy-source -v src lib",
    "commit": "git-cz",
    "flow-check": "flow check",
    "lint": "eslint src test",
    "prepublish": "yarn build",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "conventional-commits-parser": "^1.3.0",
    "find-config": "^1.0.0",
    "npm-registry-client": "^8.4.0",
    "npmlog": "^4.1.2",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "commitizen": "^2.9.6",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.49.1",
    "flow-copy-source": "^1.2.0",
    "flow-typed": "^2.1.2",
    "jest": "^20.0.4",
    "pre-commit": "^1.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
